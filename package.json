{
  "name": "openrouter-agents",
  "version": "1.2.3",
  "description": "Intelligent AI Agents for every development task using OpenRouter API",
  "keywords": [
    "openrouter",
    "ai",
    "agents",
    "cli",
    "mock-data",
    "code-generation",
    "translation",
    "documentation"
  ],
  "author": {
    "name": "Marco Bramato",
    "email": "marco@bramato.com",
    "url": "https://marco.bramato.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bramato/openrouter-agents"
  },
  "bugs": {
    "url": "https://github.com/bramato/openrouter-agents/issues"
  },
  "homepage": "https://github.com/bramato/openrouter-agents#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openrouter-agents": "./bin/openrouter-agents",
    "ora": "./bin/openrouter-agents"
  },
  "files": [
    "src",
    "dist",
    "bin",
    "config",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "figlet": "^1.7.0",
    "axios": "^1.6.2",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/jest": "^29.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}